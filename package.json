{
  "private": true,
  "name": "be",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && npm run build:page",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "build:page": "remix build",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:page\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "dev:page": "node -r dotenv/config node_modules/.bin/remix dev",
    "postinstall": "remix setup node",
    "start": "node -r dotenv/config node_modules/.bin/remix-serve build"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@netlify/functions": "^0.10.0",
    "@remix-run/netlify": "^1.1.3",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@remix-run/vercel": "^1.1.3",
    "dotenv": "^12.0.0",
    "graphql": "^16.2.0",
    "graphql-request": "^3.7.0",
    "marked": "^4.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@tailwindcss/typography": "^0.5.0",
    "@types/marked": "^4.0.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.0.0",
    "tailwindcss": "^3.0.9",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
